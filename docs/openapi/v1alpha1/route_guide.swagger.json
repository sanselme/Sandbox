{
  "swagger": "2.0",
  "info": {
    "title": "v1alpha1/route_guide.proto",
    "version": "0.1.0"
  },
  "tags": [
    {
      "name": "RouteGuide"
    }
  ],
  "consumes": ["application/json"],
  "produces": ["application/json"],
  "paths": {
    "/api/v1alpha1/sandbox/feature": {
      "get": {
        "summary": "Feature at point",
        "description": "Obtains the feature at a given position",
        "operationId": "RouteGuide_GetFeature",
        "responses": {
          "200": {
            "description": "The point where the feature is detected.",
            "schema": {
              "$ref": "#/definitions/v1alpha1Point"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "latitude",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "longitude",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          }
        ],
        "tags": ["Point"]
      }
    },
    "/api/v1alpha1/sandbox/features": {
      "get": {
        "summary": "Feature within rectangle",
        "description": "Obtains the feature within the given Rectangle",
        "operationId": "RouteGuide_ListenFeatures",
        "responses": {
          "200": {
            "description": "The point where the feature is detected.(streaming responses)",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "$ref": "#/definitions/v1alpha1Point"
                },
                "error": {
                  "$ref": "#/definitions/rpcStatus"
                }
              },
              "title": "Stream result of v1alpha1Feature"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "lo.latitude",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "lo.longitude",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "hi.latitude",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "hi.longitude",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          }
        ],
        "tags": ["Point"]
      }
    },
    "/api/v1alpha1/sandbox/notes": {
      "get": {
        "summary": "Route summary",
        "description": "Stream of RouteNotes on a route bein traversed",
        "operationId": "RouteGuide_Routechat",
        "responses": {
          "200": {
            "description": "The location from which the message is sent.(streaming responses)",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "$ref": "#/definitions/v1alpha1Point"
                },
                "error": {
                  "$ref": "#/definitions/rpcStatus"
                }
              },
              "title": "Stream result of v1alpha1RouteNote"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "location.latitude",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "location.longitude",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "message",
            "description": "The message to be sent.",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": ["Point"]
      }
    },
    "/api/v1alpha1/sandbox/summary": {
      "get": {
        "summary": "Route summary",
        "description": "Stream of Points on a route bein traversed",
        "operationId": "RouteGuide_RecordRoute",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1alpha1RouteSummary"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "latitude",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "longitude",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          }
        ],
        "tags": ["Summary"]
      }
    }
  },
  "definitions": {
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "v1alpha1Feature": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "The name of the feature."
        },
        "location": {
          "$ref": "#/definitions/v1alpha1Point",
          "description": "The point where the feature is detected."
        }
      },
      "description": "A feature names something at a given point.\nIf a feature could not be named, the name is empty."
    },
    "v1alpha1Point": {
      "type": "object",
      "properties": {
        "latitude": {
          "type": "integer",
          "format": "int32"
        },
        "longitude": {
          "type": "integer",
          "format": "int32"
        }
      },
      "description": "Points are represented as latitude-longitude pairs in the E7 representation\n(degrees multiplied by 10**7 and rounded to the nearest integer).\nLatitudes should be in the range +/- 90 degrees and longitude should be in\nthe range +/- 180 degrees (inclusive)."
    },
    "v1alpha1RouteNote": {
      "type": "object",
      "properties": {
        "location": {
          "$ref": "#/definitions/v1alpha1Point",
          "description": "The location from which the message is sent."
        },
        "message": {
          "type": "string",
          "description": "The message to be sent."
        }
      },
      "description": "A RouteNote is a message sent while at a given point."
    },
    "v1alpha1RouteSummary": {
      "type": "object",
      "properties": {
        "pointCount": {
          "type": "integer",
          "format": "int32",
          "description": "The number of points received."
        },
        "featureCount": {
          "type": "integer",
          "format": "int32",
          "description": "The number of known feature passed while traversing the route."
        },
        "distance": {
          "type": "integer",
          "format": "int32",
          "description": "The distance covered in metres."
        },
        "elapsedTime": {
          "type": "integer",
          "format": "int32",
          "description": "The duration of the traversal in seconds."
        }
      },
      "description": "A RouteSummary is received in response to a RecordRoute rpc."
    }
  },
  "externalDocs": {
    "description": "Route Guide service",
    "url": "https://github.com/sanselme/sandbox"
  }
}
